{
  "name": "tnl",
  "version": "1.0.2",
  "description": "simple secure tunnel to a local directory using ngrok and http-server",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/patmigliaccio/tnl.git"
  },
  "keywords": [
    "cli",
    "command",
    "ngrok",
    "localhost",
    "tunneling",
    "localtunnel",
    "webhook",
    "http",
    "server"
  ],
  "scripts": {
    "start": "node ./bin/tnl",
    "format": "js-beautify index.js ./bin/tnl.js ./test/tnl.spec.js -r",
    "docs": "jsdoc2md index.js > ./docs/API.md",
    "test": "node ./node_modules/mocha/bin/mocha"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "http-server": "^0.9.0",
    "js-beautify": "^1.6.11",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.2.0",
    "ngrok": "^2.2.6",
    "opener": "^1.4.3",
    "optimist": "^0.6.1",
    "portfinder": "^1.0.13",
    "yarn": "^0.21.3"
  },
  "devDependencies": {},
  "author": "patmigliaccio",
  "bugs": {
    "url": "https://github.com/patmigliaccio/tnl/issues"
  },
  "license": "MIT",
  "preferGlobal": "true",
  "bin": {
    "tnl": "./bin/tnl.js"
  }
}
